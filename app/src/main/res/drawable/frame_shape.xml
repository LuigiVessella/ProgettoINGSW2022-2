<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:width="200dp"
        android:height="200dp"
        android:top="15dp">
        <rotate
            android:fromDegrees="90"
            android:pivotX="67%"
            android:pivotY="60%">
            <shape android:shape="oval">
                <solid android:color="@color/bianco" />
                <stroke
                    android:width="5dp"
                    android:color="@color/marrone_terziario"/>
            </shape>
        </rotate>
    </item>

    <item
        android:width="310dp"
        android:height="490dp"
        android:top="40dp">
        <shape>
            <solid android:color="@color/bianco" />
            <stroke
                android:width="5dp"
                android:color="@color/marrone_terziario"/>
            <corners
                android:topLeftRadius="20dp"
                android:topRightRadius="20dp"
                android:bottomLeftRadius="20dp"
                android:bottomRightRadius="20dp" />
        </shape>

    </item>

    <item
        android:width="190dp"
        android:height="190dp"
        android:top="5dp">
        <rotate
            android:fromDegrees="90"
            android:pivotX="65.4%"
            android:pivotY="65.8%">
            <shape android:shape="oval">
                <solid android:color="@color/bianco" />
            </shape>
        </rotate>
    </item>
</layer-list>