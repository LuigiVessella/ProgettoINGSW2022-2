<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:width="20dp"
        android:height="20dp"
        android:top="12dp"
        android:right="2dp"
        android:left="0.3dp">
        <rotate
            android:fromDegrees="90"
            android:pivotX="80%"
            android:pivotY="45%">
            <shape android:shape="oval">
                <solid android:color="@color/bianco" />
                <stroke
                    android:width="0.9dp"
                    android:color="@color/marrone_terziario"/>
            </shape>
        </rotate>
    </item>

    <item
        android:width="31dp"
        android:height="50dp"
        android:bottom="4dp"
        android:top="12dp">
        <shape>
            <solid android:color="@color/bianco" />
            <stroke
                android:width="0.7dp"
                android:color="@color/marrone_terziario"/>
            <corners
                android:topLeftRadius="4dp"
                android:topRightRadius="4dp"
                android:bottomLeftRadius="4dp"
                android:bottomRightRadius="4dp" />
        </shape>

    </item>

    <item
        android:width="20.13dp"
        android:height="20dp"
        android:top="14.5dp"
        android:right="2dp"
        android:left="0.1dp">
        <rotate
            android:fromDegrees="90"
            android:pivotX="80%"
            android:pivotY="45%">
            <shape android:shape="oval">
                <solid android:color="@color/bianco" />
                <stroke
                    android:width="10dp"
                    android:color="@color/bianco"/>
            </shape>
        </rotate>
    </item>

</layer-list>